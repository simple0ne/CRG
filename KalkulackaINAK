#include <iostream>
using namespace std;

int main() {
  int x, y;
  int suma;
  cout <<"Napis cislo:";
  cin >> x;
  cout <<"Napis dalsie cislo: ";
  cin >> y;
  suma = x + y;
  cout<< "Vysledok scitania je: " << suma;
  suma = x - y;
  cout <<"Vysledok odcitania je:" << suma;
  suma = x * y;
  cout <<"Vysledok nasobenia je:" << suma;
  suma = x / y;
  cout <<"Vysledok delenia je:" << suma;
  return 0;
}
